#include<iostream>
using namespace std;
#define ll long long int
ll sumtilln(ll n)
{
    ll sum=0;
    for(ll i=0;i<=n;i++)
    {
        sum+=i;
    }
    return sum;
}
int main(void)
{
    ll n;
    cin>>n;
    ll a[n];
    ll sum=0;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        sum+=a[i];
    }
    ll x= sumtilln(n);
    if(x==sum)
        cout<<"YES"<<endl;
    else
        cout<<"NO"<<endl;
}
